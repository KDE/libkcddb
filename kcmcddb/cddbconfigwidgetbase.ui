<!DOCTYPE UI><UI version="3.0" stdsetdef="1">
<class>CDDBConfigWidgetBase</class>
<comment>Used for configuring libkcddb.</comment>
<widget class="QWidget">
    <property name="name">
        <cstring>CDDBConfigWidgetBase</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>396</width>
            <height>230</height>
        </rect>
    </property>
    <property name="caption">
        <string>CDDB Settings</string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>11</number>
        </property>
        <property name="spacing">
            <number>6</number>
        </property>
        <widget class="QButtonGroup">
            <property name="name">
                <cstring>cachePolicy</cstring>
            </property>
            <property name="title">
                <string>Mode:</string>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>5</hsizetype>
                    <vsizetype>4</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <hbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>cacheOnly</cstring>
                    </property>
                    <property name="text">
                        <string>Cache Only</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Check only in the local cache for CD info.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>cacheAndRemote</cstring>
                    </property>
                    <property name="text">
                        <string>Cache and Remote</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Check for locally cached CD info before trying to look up at remote CDDB server.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>remoteOnly</cstring>
                    </property>
                    <property name="text">
                        <string>Remote only</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Only try to look up at remote CDDB server.</string>
                    </property>
                </widget>
            </hbox>
        </widget>
        <widget class="QGroupBox">
            <property name="name">
                <cstring>serverBox</cstring>
            </property>
            <property name="title">
                <string>CDDB Server</string>
            </property>
            <grid>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>11</number>
                </property>
                <property name="spacing">
                    <number>6</number>
                </property>
                <widget class="QLineEdit" row="0" column="1">
                    <property name="name">
                        <cstring>cddbServer</cstring>
                    </property>
                    <property name="text">
                        <string>freedb.freedb.org</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Name of CDDB server which will be used to look up CD information.</string>
                    </property>
                </widget>
                <widget class="QSpinBox" row="0" column="3">
                    <property name="name">
                        <cstring>cddbPort</cstring>
                    </property>
                    <property name="maxValue">
                        <number>64000</number>
                    </property>
                    <property name="value">
                        <number>8880</number>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Port to connect to on CDDB server.</string>
                    </property>
                </widget>
                <widget class="QLabel" row="0" column="0">
                    <property name="name">
                        <cstring>TextLabel7</cstring>
                    </property>
                    <property name="text">
                        <string>CDDB &amp;server:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>cddbServer</cstring>
                    </property>
                </widget>
                <widget class="QLabel" row="0" column="2">
                    <property name="name">
                        <cstring>TextLabel8</cstring>
                    </property>
                    <property name="text">
                        <string>Port:</string>
                    </property>
                    <property name="alignment">
                        <set>AlignVCenter|AlignRight</set>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>cddbPort</cstring>
                    </property>
                </widget>
                <widget class="QLabel" row="1" column="0">
                    <property name="name">
                        <cstring>TextLabel9</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Transport:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>cddbType</cstring>
                    </property>
                </widget>
                <widget class="QComboBox" row="1" column="1" rowspan="1" colspan="3">
                    <item>
                        <property name="text">
                            <string>CDDB</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>HTTP</string>
                        </property>
                    </item>
                    <property name="name">
                        <cstring>cddbType</cstring>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Type of lookup which should be tried at the CDDB server.</string>
                    </property>
                </widget>
            </grid>
        </widget>
        <widget class="QGroupBox">
            <property name="name">
                <cstring>GroupBox1</cstring>
            </property>
            <property name="title">
                <string>Submissions</string>
            </property>
            <grid>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>11</number>
                </property>
                <property name="spacing">
                    <number>6</number>
                </property>
                <widget class="QCheckBox" row="0" column="0" rowspan="1" colspan="3">
                    <property name="name">
                        <cstring>submissionsEnable</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Enable submissions</string>
                    </property>
                    <property name="checked">
                        <bool>true</bool>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Allow submission of CD info to a CDDB server. Enabling this makes you a good netizen.</string>
                    </property>
                </widget>
                <widget class="QLineEdit" row="2" column="1" rowspan="1" colspan="3">
                    <property name="name">
                        <cstring>submissionsSendTo</cstring>
                    </property>
                    <property name="text">
                        <string>freedb-submit@freedb.org</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Email address which will be used for SMTP CD info submissions.</string>
                    </property>
                </widget>
                <widget class="QLabel" row="2" column="0">
                    <property name="name">
                        <cstring>TextLabel3</cstring>
                    </property>
                    <property name="text">
                        <string>Emai&amp;l address:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>submissionsSendTo</cstring>
                    </property>
                </widget>
            </grid>
        </widget>
        <spacer>
            <property name="name" stdset="0">
                <cstring>Spacer6</cstring>
            </property>
            <property name="orientation">
                <enum>Vertical</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>20</width>
                    <height>20</height>
                </size>
            </property>
        </spacer>
    </vbox>
</widget>
<connections>
    <connection>
        <sender>cacheOnly</sender>
        <signal>toggled(bool)</signal>
        <receiver>serverBox</receiver>
        <slot>setDisabled(bool)</slot>
    </connection>
    <connection>
        <sender>cacheAndRemote</sender>
        <signal>toggled(bool)</signal>
        <receiver>serverBox</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>remoteOnly</sender>
        <signal>toggled(bool)</signal>
        <receiver>serverBox</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>submissionsEnable</sender>
        <signal>toggled(bool)</signal>
        <receiver>TextLabel3</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>submissionsEnable</sender>
        <signal>toggled(bool)</signal>
        <receiver>submissionsSendTo</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>cddbServer</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>CDDBConfigWidgetBase</receiver>
        <slot>slotConfigChanged()</slot>
    </connection>
    <connection>
        <sender>cddbPort</sender>
        <signal>valueChanged(int)</signal>
        <receiver>CDDBConfigWidgetBase</receiver>
        <slot>slotConfigChanged()</slot>
    </connection>
    <connection>
        <sender>cddbType</sender>
        <signal>activated(int)</signal>
        <receiver>CDDBConfigWidgetBase</receiver>
        <slot>slotConfigChanged()</slot>
    </connection>
    <connection>
        <sender>submissionsEnable</sender>
        <signal>toggled(bool)</signal>
        <receiver>CDDBConfigWidgetBase</receiver>
        <slot>slotConfigChanged()</slot>
    </connection>
    <connection>
        <sender>submissionsSendTo</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>CDDBConfigWidgetBase</receiver>
        <slot>slotConfigChanged()</slot>
    </connection>
    <connection>
        <sender>cacheOnly</sender>
        <signal>toggled(bool)</signal>
        <receiver>CDDBConfigWidgetBase</receiver>
        <slot>slotConfigChanged()</slot>
    </connection>
    <connection>
        <sender>cacheAndRemote</sender>
        <signal>toggled(bool)</signal>
        <receiver>CDDBConfigWidgetBase</receiver>
        <slot>slotConfigChanged()</slot>
    </connection>
    <connection>
        <sender>remoteOnly</sender>
        <signal>toggled(bool)</signal>
        <receiver>CDDBConfigWidgetBase</receiver>
        <slot>slotConfigChanged()</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>cacheOnly</tabstop>
    <tabstop>cacheAndRemote</tabstop>
    <tabstop>remoteOnly</tabstop>
    <tabstop>cddbServer</tabstop>
    <tabstop>cddbPort</tabstop>
    <tabstop>cddbType</tabstop>
    <tabstop>submissionsEnable</tabstop>
    <tabstop>submissionsSendTo</tabstop>
</tabstops>
<slots>
    <slot access="protected">slotConfigChanged()</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
</UI>
